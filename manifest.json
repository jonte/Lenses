{
  "name": "Lense",
  "version": "0.1",
  "description": "Provides different perspectives on text selected in the current tab.",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["http://*/*",
                  "https://*/*"],
      "js": ["page.js"]
    }
  ],
  "browser_action": {
    "default_title": "Open Lense",
    "default_icon": "lense.png",
    "popup": "popup.html"
  }
}
